<h1>Log In</h1>

<%if @error%>
  <h3>This user does not exist.</h3>
<%end%>

<%=form_tag "/login" do%>
<%=label_tag "Username" %>
<%=text_field_tag :username%>
<br><br>
<%=label_tag "Password" %>
<%=password_field_tag :password%>
<br><br>
<%=submit_tag :submit%>

<%end%>

<br>

<%= link_to "Create a new user", new_owner_path %>