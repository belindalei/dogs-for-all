    <body class="catpage">
        <div>
        <center>

        <h1>All Cats Available For Adoption</h1>
        <h3>Number of cats available for adoption:
            <strong><%= @cats.count %></strong></h3>

        <% @cats.each do |cat|%>
            <p><%= link_to cat.name, pet_path(cat) %></p>
        <% end %>

        <%= button_to "Adopt A Cat", adopt_cat_path(), class:"button", method: :get %>



        </center>
        </div>
    </body>
