
<body class="petsindex">
<div>
<center>

<h1>See all of our pets available for adoption here:</h1>

  <% @pets.each do |pet| %>
    <h3><%= link_to pet.name, pet_path(pet) %></h3>
  <% end %>

  <%= button_to "Adopt", adoptions_path(), class:"button", method: :get %>
    <br><br>
  <%= button_to "Return to Home Page", owner_path(@owner), class:"button_two", method: :get %>

</center>
</div>
</body>
