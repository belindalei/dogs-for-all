<body class="dogpage">
    <center>

    <h1>All Dogs Available For Adoption</h1>


    <% @dogs.each do |dog|%>
        <p><%= link_to dog.name, pet_path(dog) %><p>
    <% end %>

    <%= button_to "Adopt A Dog", adopt_dog_path(), class:"button", method: :get %>
            <br><br>
    <%= button_to "Return to Home Page", owner_path(@owner), class:"button_two", method: :get %>


    </center>
</body>